{
    "description": "Sandstorm KISSearch - Neos extension - search your content",
    "type": "neos-package",
    "require": {
        "neos/flow": "~9.0",
        "neos/neos": "~9.0",
        "sandstorm/kissearch-api": "@dev"
    },
    "license": "MIT",
    "name": "sandstorm/kissearch-neos",
    "autoload": {
        "psr-4": {
            "Sandstorm\\KISSearch\\Neos\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sandstorm\\KISSearch\\Neos\\Tests\\": "Tests/"
        }
    },
    "archive": {
        "exclude": [
        ]
    },
    "extra": {
        "neos": {
            "package-key": "Sandstorm.KISSearch.Neos"
        }
    }
}
